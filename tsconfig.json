{
  "extends": "@nuxt/typescript",
  "compilerOptions": {
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": false,
    "baseUrl": ".",
    "strict": false,
    "allowJs": true,
    "resolveJsonModule": true,
    "types": [
      "@types/node",
      "@types/webpack-env",
      "@nuxt/vue-app"
    ],
    "paths": {
      "~/*": ["./*"]
    }
  }
}
